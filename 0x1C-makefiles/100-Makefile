# make -f 100-Makefile

CC= gcc
CFLAGS= -Wall -Werror -Wextra -pedantic
SRC= main.c school.c
OBJ= $(SRC:.c=.o)
NAME= school

all: $(NAME)

$(NAME): $(OBJ)
        $(CC) -o $@ $^

clean:
	$(RM) *~ $(NAME)

oclean:
	$(RM) $(OBJ)

fclean: clean oclean

re: fclean all

%.o: %.c  m.h
	$(CC) -c -o $@ $<

.PHONY: all clean oclean fclean re
